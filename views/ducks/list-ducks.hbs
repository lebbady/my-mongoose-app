<h1>
  Look at all my ducks!
</h1>

<ul>
  {{#each ducks}}
  <li>
    <h2>{{this.name}}</h2>
    <p>{{this.year}}</p>
    <p>{{this.type}}</p>
    <a href="/ducks/{{this._id}}/edit">Edit this duck</a>
    <form action="/ducks/{{this._id}}/delete" method="post">
      <div>
        <button type="submit">Kill the duck</button>
      </div>
    </form>
  </li>
  {{/each}}
</ul>